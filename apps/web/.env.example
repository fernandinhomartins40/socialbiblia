# Configurações de Exemplo - Biblicai Frontend
# Copie este arquivo para .env.local para desenvolvimento

# Ambiente
NODE_ENV=development

# URLs da API para desenvolvimento
VITE_API_URL=http://localhost:3000/api
VITE_WS_URL=http://localhost:3000/socket.io

# Configurações do Plugbase Backend
VITE_BACKEND_URL=http://localhost:3000

# Configurações de Build
VITE_BUILD_MODE=development

# Configurações de Cache
VITE_ENABLE_PWA=false

# Debug (habilitado em desenvolvimento)
VITE_DEBUG=true

# Configurações opcionais
# VITE_CUSTOM_API_TIMEOUT=10000
# VITE_ENABLE_MOCK_DATA=false