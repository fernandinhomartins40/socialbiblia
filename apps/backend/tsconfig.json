{
  "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs",
    "lib": ["ES2022", "dom"],
    "types": ["node"],
    "outDir": "./dist",
    "rootDir": "./",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "removeComments": true,
    "strictNullChecks": false,
    "strictFunctionTypes": false,
    "noImplicitThis": false,
    "noImplicitReturns": false,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedIndexedAccess": false,
    "noImplicitOverride": false,
    "allowUnusedLabels": false,
    "allowUnreachableCode": false,
    "exactOptionalPropertyTypes": false,
    "baseUrl": "./src",
    "paths": {
      "@utils/*": ["utils/*"],
      "@services/*": ["services/*"],
      "@middlewares/*": ["middlewares/*"],
      "@controllers/*": ["controllers/*"],
      "@routes/*": ["routes/*"],
      "@dao/*": ["dao/*"],
      "@functions/*": ["functions/*"],
      "@config/*": ["config/*"],
      "@constants/*": ["constants/*"],
      "@schemas/*": ["schemas/*"],
      "@core/*": ["core/*"],
      "@types/*": ["types/*"],
      "@models/*": ["models/*"],
      "@modules/*": ["modules/*"],
      "@database/*": ["database/*"],
      "@server/*": ["server/*"],
      "@packagejson": ["../package.json"]
    }
  },
  "include": ["src/**/*", "package.json", "prisma/**/*"],
  "exclude": ["node_modules", "dist", "tests"]
}
