{
  "name": "plugbase-backend",
  "version": "1.0.0",
  "description": "Backend plugável, enterprise-ready e reutilizável",
  "main": "dist/app.js",
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "prebuild": "npm run db:generate",
    "build": "tsc",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{ejs,html,json,sql}\" dist/",
    "start": "node dist/app.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:seed": "tsx prisma/seed.ts",
    "prisma:reset:force": "prisma migrate reset --force",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "plugbase": "tsx src/cli/plugbase-cli.ts"
  },
  "bin": {
    "plugbase": "./dist/cli/plugbase-cli.js"
  },
  "keywords": [
    "backend",
    "plugável",
    "modular",
    "enterprise",
    "typescript"
  ],
  "author": "Plugbase Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@types/redis": "^4.0.10",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "bcrypt": "^5.1.1",
    "commander": "^11.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "multer": "^1.4.5-lts.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^5.7.1",
    "redis": "^5.6.1",
    "socket.io": "^4.7.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/supertest": "^6.0.3",
    "commander": "^11.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^30.0.5",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
