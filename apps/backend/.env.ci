# CI/CD Environment Variables for Build and Validation
DATABASE_URL="postgresql://ci_user:ci_password@localhost:5432/ci_database"

# App Configuration for CI
APP_URL_HOST=0.0.0.0
APP_URL_PORT=3344
SSL_ALLOW=false
API_PREFIX=api
API_JSON_LIMIT=5mb
API_EXT_URLENCODED=false
CORS_ALLOW_ORIGIN=*

# JWT Configuration for CI
JWT_SECRET_USER=ci_jwt_secret_user_for_validation
JWT_SECRET_DEVICE=ci_jwt_secret_device_for_validation
JWT_EXPIRED_IN=24h

# Security for CI
BCRYPT_SALTROUNDS=10
RATE_LIMIT_MAX=1000
RATE_LIMIT_WINDOW=15

# Debug for CI
DEBUG_HTTP_REQUEST=false
DEBUG_HTTP_CONNECTION=false

# Node Environment
NODE_ENV=test